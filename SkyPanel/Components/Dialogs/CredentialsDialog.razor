@inject ISnackbar Snackbar

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.body1">@Username</MudText>
        <MudText Typo="Typo.body1">@Password</MudText>
      
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="@DialogCancel" Variant="Variant.Filled">Cancel</MudButton>
        <MudSpacer/>
        <MudButton Color="@Color.Primary" OnClick="@DialogSubmit" Variant="Variant.Filled">Confirm</MudButton>
    </DialogActions>
</MudDialog>