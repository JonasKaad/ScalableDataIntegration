@page "/"
<PageTitle>SkyPanel — Dashboard</PageTitle>
<AuthorizeView>
    <Authorized>
        <MudText></MudText>
        <MudText Typo="Typo.h6">Hello, @context.User.Identity?.Name</MudText>
        <MudButton Href="Account/Logout" Color="Color.Error" Variant="Variant.Filled">Log out</MudButton>
<Dashboard />
    </Authorized>
    <NotAuthorized>
        <MudButton Href="Account/Login" Color="Color.Primary" Variant="Variant.Filled">Log in</MudButton>
    </NotAuthorized>
</AuthorizeView>
